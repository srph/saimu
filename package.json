{
  "name": "saimu",
  "scripts": {
    "build:script": "node_modules/.bin/webpack --config=build/webpack.config.js --progress --display-error-details",
    "watch:script": "node_modules/.bin/webpack --config=build/webpack.config.js --progress -w --display-error-details",
    "copy:fonts": "cp -r node_modules/font-awesome/fonts/* main/fonts/",
    "prod:script": "node_modules/.bin/webpack --config=build/webpack.config.js --progress -p --verbose --display-error-details",
    "build:style": "node-sass src/styles/style.scss -o main/dist && npm run copy:fonts",
    "watch:style": "npm run build:style && node-sass -w src/styles/style.scss -o main/dist",
    "post:style": "postcss -u autoprefixer --autoprefixer.browsers '> 5%' -u cssnano -o main/dist/style.css main/dist/style.css && npm run copy:fonts",
    "prod:style": "npm run build:style && npm run post:style",
    "prod": "npm run prod:script && npm run prod:style",
    "electron": "electron main/index.js",
    "start": "npm-run-all --parallel watch:script watch:style electron"
  },
  "dependencies": {
    "axios": "^0.15.0",
    "classnames": "^2.2.0",
    "cookie-machine": "^0.1.1",
    "electron": "^1.6.2",
    "font-awesome": "^4.7.0",
    "history": "^2.1.2",
    "lodash": "^4.17.4",
    "lodash-id": "^0.14.0",
    "lowdb": "^0.16.0",
    "numeral": "^2.0.5",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-gateway": "^2.6.0",
    "react-helmet": "^2.1.1",
    "react-modal2": "^3.1.0",
    "react-redux": "^4.0.0",
    "react-resolver": "^2.0.5",
    "react-router": "^2.8.1",
    "react-router-scroll": "^0.3.2",
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.0",
    "tinytime": "^0.2.5"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "cssnano": "^3.5.2",
    "dotenv": "^1.2.0",
    "dotenv-autoload": "^0.1.3",
    "electron-prebuilt": "^1.4.13",
    "node-sass": "^3.4.2",
    "npm-run-all": "^1.4.0",
    "postcss": "^5.0.19",
    "postcss-cli": "^2.5.1",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "stringify-object-values": "^0.1.0",
    "webpack": "2.1.0-beta.25",
    "webpack-target-electron-renderer": "^0.4.0"
  }
}
